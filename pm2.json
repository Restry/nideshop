{
  "name": "windart",
  "script": "production.js",
  "cwd": "./",
  "watch": [
    "./"
  ],
  "exec_mode": "fork",
  "instances": 4,
  "ignore_watch": [
    "node_modules",
    "logs"
  ],
  "error_file": "./logs/app-err.log",
  "out_file": "./logs/app-out.log",
  "env": {
    "NODE_ENV": "production"
  }
}
