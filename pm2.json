{
  "apps": [{
    "name": "windart",
    "script": "production.js",
    "cwd": "./",
    "exec_mode": "cluster_mode",
    "max_memory_restart": "1G",
    "autorestart": true,
    "node_args": [],
    "instances": 4,
    "ignore_watch": [
      "node_modules",
      "logs"
    ],
    "error_file": "./logs/app-err.log",
    "out_file": "./logs/app-out.log",
    "args": [],
    "env": {
      "NODE_ENV": "production"
    }
  }]
}
